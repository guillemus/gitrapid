{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        { "source": "/", "destination": "/index.html" },
        {
            "source": "/:path((?!api/).*)",
            "has": [{ "type": "header", "key": "Accept", "value": ".*text/html.*" }],
            "destination": "/app/index.html"
        }
    ]
}
