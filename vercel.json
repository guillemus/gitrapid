{
  "headers": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "header", "key": "Accept", "value": ".*text/html.*" }
      ],
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        { "key": "CDN-Cache-Control", "value": "public, s-maxage=31536000, stale-while-revalidate=60" }
      ]
    }
  ]
}