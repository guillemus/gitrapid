{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "./convex/**/*", "./src/**/*", "./scripts/**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "jsx": "react-jsx",
        "noUncheckedIndexedAccess": true,
        "noImplicitThis": true,
        "jsxImportSource": "react",
        "paths": {
            "@/*": ["./src/*"],
            "@convex/*": ["./convex/*"],
        }
    }
}