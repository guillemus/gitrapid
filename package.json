{
  "name": "github-file-browser",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently -c \"cyan.bold,green.bold\" -n \"CONVEX,ASTRO \" \"bun cx\" \"bun astro:dev\"",
    "cx": "convex dev --typecheck disable",
    "ts": "tsgo --watch",
    "ts:build": "tsgo",
    "astro:dev": "astro dev --port 3000",
    "scripts": "NO_COLOR=1 bun run scripts/scripts.ts | tee >(pbcopy)",
    "dev:tsc": "tsc --watch",
    "build": "astro build",
    "lint": "eslint ./src ./convex",
    "format": "prettier ./src ./convex --write",
    "preview": "astro preview --port 3000",
    "start": "node dist/server/entry.mjs",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/node": "9.3.3",
    "@astrojs/react": "4.3.0",
    "@astrojs/vercel": "^8.2.5",
    "@auth/core": "0.40.0",
    "@convex-dev/aggregate": "^0.1.21",
    "@convex-dev/auth": "^0.0.88",
    "@convex-dev/migrations": "^0.2.9",
    "@convex-dev/react-query": "0.0.0-alpha.11",
    "@fontsource/geist-sans": "5.2.5",
    "@primer/octicons-react": "^19.15.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@shikijs/transformers": "3.9.1",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/react-query": "5.84.1",
    "@tanstack/react-query-devtools": "5.84.1",
    "@tanstack/react-query-persist-client": "5.84.1",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@typescript/native-preview": "^7.0.0-dev.20250819.1",
    "astro": "5.12.8",
    "better-auth": "1.3.4",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.25.4",
    "convex-helpers": "^0.1.104",
    "dompurify": "3.2.6",
    "dotenv": "17.2.1",
    "github-markdown-css": "5.8.1",
    "idb-keyval": "6.2.2",
    "lucide-react": "^0.536.0",
    "marked": "16.1.1",
    "octokit": "^5.0.3",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "5.5.0",
    "react-markdown": "10.1.0",
    "react-router": "7.7.1",
    "shiki": "3.9.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "4.1.11",
