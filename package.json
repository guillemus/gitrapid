{
  "name": "github-file-browser",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently -c \"cyan.bold,green.bold\" -n \"CONVEX,ASTRO \" \"bun cx\" \"bun astro:dev\"",
    "cx": "convex dev --typecheck disable",
    "ts": "tsgo --watch",
    "build": "astro build",
    "lint": "oxlint --type-aware ./convex ./src",
    "scripts": "bun run scripts/scripts.ts",
    "scripts:debug": "bun tsx --inspect scripts/scripts.ts",
    "truncate": "bun run scripts/cmd/truncate.ts",
    "data": "bun run scripts/cmd/data.ts",
    "astro:dev": "astro dev --port 3000",
    "format": "prettier ./src ./convex --write --config .prettierrc-org-imports",
    "astro": "astro",
    "vitest": "vitest run --test-timeout=0 --no-file-parallelism",
    "vitest:debug": "vitest run --test-timeout=0 --inspect --inspect-brk --no-file-parallelism",
    "migrations:runIt": "convex run convex/migrations.ts:runIt",
    "migrations:watch": "convex run --component migrations --watch lib:getStatus"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "4.4.0",
    "@astrojs/vercel": "^8.2.8",
    "@convex-dev/aggregate": "^0.1.24",
    "@convex-dev/auth": "^0.0.90",
    "@convex-dev/migrations": "^0.2.9",
    "@convex-dev/react-query": "0.0.0-alpha.11",
    "@convex-dev/workflow": "^0.2.6",
    "@convex-dev/workpool": "^0.2.18",
    "@edge-runtime/vm": "^5.0.0",
    "@fontsource/geist-sans": "5.2.5",
    "@octokit/graphql": "^9.0.2",
    "@primer/octicons-react": "^19.18.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@shikijs/transformers": "3.13.0",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-query-devtools": "5.90.2",
    "@tanstack/react-query-persist-client": "5.90.2",
    "astro": "5.14.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.27.3",
    "convex-helpers": "^0.1.104",
    "date-fns": "^4.1.0",
    "dompurify": "3.2.7",
    "dotenv": "17.2.3",
    "idb-keyval": "6.2.2",
    "lucide-react": "^0.544.0",
    "marked": "16.3.0",
    "marked-gfm-heading-id": "^4.1.2",
    "octokit": "^5.0.3",
    "pino": "^9.12.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "5.5.0",
    "react-markdown": "10.1.0",
    "react-router": "7.9.3",
    "shiki": "3.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "typescript": "5.9.3",
    "valtio": "2.1.8",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@types/bun": "^1.2.23",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@typescript/native-preview": "^7.0.0-dev.20251001.1",
    "@eslint/js": "9.36.0",
    "@octokit/types": "^15.0.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/dotenv": "^8.2.3",
    "concurrently": "^9.2.1",
    "convex-test": "^0.0.38",
    "dotenv-cli": "^10.0.0",
    "globals": "16.4.0",
    "oxlint": "^1.19.0",
    "oxlint-tsgolint": "^0.2.0",
    "pino-pretty": "^13.1.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript-eslint": "^8.45.0",
    "vercel": "^48.1.6",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
