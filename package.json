{
  "name": "github-file-browser",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently -c \"cyan.bold,green.bold\" -n \"CONVEX,ASTRO \" \"bun cx\" \"bun astro:dev\"",
    "cx": "convex dev --typecheck disable",
    "ts": "tsgo --watch",
    "build": "astro build",
    "lint": "eslint ./convex ./src",
    "lint:no-circular": "dpdm --no-warning --no-tree ./convex/** ./src/**",
    "scripts": "bun run scripts/scripts.ts",
    "scripts:debug": "bun tsx --inspect scripts/scripts.ts",
    "truncate": "bun run scripts/cmd/truncate.ts",
    "astro:dev": "astro dev --port 3000",
    "format": "prettier ./src ./convex --write --config .prettierrc-org-imports",
    "test": "vitest",
    "astro": "astro",
    "migrations:runIt": "convex run convex/migrations.ts:runIt",
    "migrations:watch": "convex run --component migrations --watch lib:getStatus"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/node": "9.4.3",
    "@astrojs/react": "4.3.1",
    "@astrojs/vercel": "^8.2.7",
    "@auth/core": "0.40.0",
    "@convex-dev/aggregate": "^0.1.23",
    "@convex-dev/auth": "^0.0.88",
    "@convex-dev/migrations": "^0.2.9",
    "@convex-dev/react-query": "0.0.0-alpha.11",
    "@edge-runtime/vm": "^5.0.0",
    "@fontsource/geist-sans": "5.2.5",
    "@primer/octicons-react": "^19.17.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@shikijs/transformers": "3.12.2",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.13",
    "@tanstack/react-query": "5.87.1",
    "@tanstack/react-query-devtools": "5.87.3",
    "@tanstack/react-query-persist-client": "5.87.1",
    "@types/bun": "^1.2.21",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@typescript/native-preview": "^7.0.0-dev.20250909.1",
    "astro": "5.13.6",
    "better-auth": "1.3.9",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.26.2",
    "convex-helpers": "^0.1.104",
    "date-fns": "^4.1.0",
    "dompurify": "3.2.6",
    "dotenv": "17.2.2",
    "idb-keyval": "6.2.2",
    "lucide-react": "^0.543.0",
    "marked": "16.2.1",
    "marked-gfm-heading-id": "^4.1.2",
    "octokit": "^5.0.3",
    "p-limit": "^7.1.1",
    "pg": "^8.16.3",
    "pino": "^9.9.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "5.5.0",
    "react-markdown": "10.1.0",
    "react-router": "7.8.2",
    "shiki": "3.12.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2",
    "valtio": "2.1.7",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@octokit/types": "^14.1.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/dotenv": "^8.2.3",
    "concurrently": "^9.2.1",
    "convex-test": "^0.0.38",
    "dotenv-cli": "^10.0.0",
    "dpdm": "^3.14.0",
    "eslint": "9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "16.3.0",
    "pino-pretty": "^13.1.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.8",
    "typescript-eslint": "^8.43.0",
    "vercel": "^47.0.5",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
